{
    "name": "phantombot-youtube-nodecg",
    "version": "0.0.1",
    "description": "Phantombot Youtube Implementation for nodecg",
    "main": "index.js",
    "author": {
        "name": "JKSessions",
        "email": "jksessions@jksessions.id.au",
        "url": "http://jksessions.id.au/"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/jksessions/phantombot-youtube-nodecg"
    },
    "nodecg": {
        "compatibleRange": "^1.0.0",
        "graphics": [
            {
                "file": "youtube.html",
                "width": 1920,
                "height": 1080,
                "singleInstance": true
            }
        ],
        "dashboardPanels": [
            {
                "name": "youtube-controls",
                "title": "Youtube Controls",
                "width": 2,
                "file": "youtube-controls.html",
                "headerColor": "#FF0000",
                "workspace": "My Workspace"
            },
            {
                "name": "youtube-controls-v2",
                "title": "Youtube Controls",
                "width": 4,
                "file": "youtube-controls-v2.html",
                "headerColor": "#FF0000",
                "workspace": "My Workspace"
            },
            {
                "name": "youtube-playlist",
                "title": "Youtube Playlist",
                "width": 4,
                "file": "youtube-playlist.html",
                "headerColor": "#FF0000",
                "workspace": "My Workspace"
            },
            {
                "name": "youtube-queuelist",
                "title": "Youtube Queue",
                "width": 3,
                "file": "youtube-queuelist.html",
                "headerColor": "#FF0000",
                "workspace": "My Workspace"
            },
            {
                "name": "add-song-to-playlist",
                "title": "Add Song to Playlist",
                "width": 3,
                "dialog": true,
                "dialogButtons": [
                    {
                        "name": "save",
                        "type": "confirm"
                    }
                ],
                "file": "add-song-to-playlist.html"
            },
            {
                "name": "change-playlist",
                "title": "Change Playlist",
                "width": 3,
                "dialog": true,
                "dialogButtons": [
                    {
                        "name": "save",
                        "type": "confirm"
                    }
                ],
                "file": "change-playlist.html"
            },
            {
                "name": "add-song-to-queue",
                "title": "Add Song to Queue",
                "width": 3,
                "dialog": true,
                "dialogButtons": [
                    {
                        "name": "save",
                        "type": "confirm"
                    }
                ],
                "file": "add-song-to-queue.html"
            },
            {
                "name": "request-settings",
                "title": "Request Settings",
                "width": 3,
                "dialog": true,
                "dialogButtons": [
                    {
                        "name": "save",
                        "type": "confirm"
                    }
                ],
                "file": "request-settings.html"
            }
        ]
    },
    "dependencies": {
        "@polymer/iron-icons": "^3.0.1",
        "@polymer/paper-button": "^3.0.1",
        "@polymer/paper-dropdown-menu": "^3.2.0",
        "@polymer/paper-input": "^3.2.1",
        "@polymer/paper-item": "^3.0.1",
        "@polymer/paper-listbox": "^3.0.1",
        "@polymer/paper-slider": "^3.0.1",
        "@polymer/paper-styles": "^3.0.1",
        "@polymer/paper-toggle-button": "^3.0.1",
        "@polymer/polymer": "^3.4.1",
        "@webcomponents/webcomponentsjs": "^2.5.0",
        "ws": "^7.4.6",
        "nodecg": "^1.7.4"
    },
    "devDependencies": {
        "@types/deep-equal": "^1.0.1",
        "@types/lodash": "^4.14.168",
        "@types/node-fetch": "^2.5.8",
        "@types/react-beautiful-dnd": "^13.0.0",
        "@types/react-dom": "^17.0.0",
        "@types/styled-components": "^5.1.7",
        "@types/uuid": "^8.3.0",
        "@types/ws": "^7.4.0",
        "@typescript-eslint/eslint-plugin": "^4.15.0",
        "@typescript-eslint/parser": "^4.15.0",
        "eslint": "^7.19.0",
        "eslint-config-prettier": "^7.2.0",
        "eslint-config-react": "^1.1.7",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-react": "^7.22.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "prettier": "^2.2.1"
    }
}
